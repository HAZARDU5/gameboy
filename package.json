{
  "name": "aframe-gameboy",
  "version": "1.0.0",
  "description": "JavaScript GameBoy Emulator for WebVR",
  "main": "index.js",
  "scripts": {
    "start": "budo --live --verbose --port 3000 --open",
    "test": "echo \"Error: no test specified\" && exit 1",
    "deploy": "npm run ghpages",
    "ghpages": "npm run dist && ./node_modules/.bin/gh-pages -d _dist -b gh-pages",
    "dist": "mkdir _dist; cp -R js _dist && cp -R assets _dist && cp -R css _dist && cp index.html _dist"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hazardu5/gameboy.git"
  },
  "author": "Michael Andrew, Afonso F, Grant Galitz",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/hazardu5/gameboy/issues"
  },
  "homepage": "https://github.com/afonsof/gameboy#readme",
  "devDependencies": {
    "budo": "^10.0.3",
    "gh-pages": "^1.0.0"
  }
}
